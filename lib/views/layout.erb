<!DOCTYPE html>
<html lang="<%= I18n.locale.to_s %>" class="<%= Helper::Toggles.enabled?("frontend-govuk-rebrand", default: true) ? 'govuk-template--rebranded' : 'govuk-template' %>">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
  <meta name="theme-color" content="#0b0c0c">
  <meta name="robots" content="noindex, nofollow">
  <meta name="google-site-verification" content="3lgiSwUvqRfNZ-lZOvb5S6BPeZYf8uK2YT1yGZqiedE" />
  <!--[if gt IE 8]><!-->
  <link href="<%= assets_path('/application.css') %>" media="all" rel="stylesheet"
        type="text/css">
  <!--<![endif]-->

  <title><%=@page_title || "#{t('layout.head.title')} â€“ #{t('layout.body.govuk')}"%></title>

  <!-- All this for a Favicon... -->
  <% if !Helper::Toggles.enabled?("frontend-govuk-rebrand", default: true) %>
    <link rel="shortcut icon" type="image/x-icon" sizes="16x16 32x32 48x48" href="<%= assets_path('/images/favicon.ico') %>" />
    <link rel="icon" sizes="any" href="<%= assets_path('/images/favicon.svg') %>" type="image/svg+xml">
    <link rel="mask-icon" href="<%= assets_path('/images/govuk-icon-mask.svg') %>" color="#0b0c0c">
    <link rel="apple-touch-icon" href="<%= assets_path('/images/govuk-icon-180.png') %>">
    <link rel="manifest" href="<%= assets_path('/manifest.json') %>">
  <% else %>
      <link rel="shortcut icon" type="image/x-icon" sizes="16x16 32x32 48x48" href="<%= assets_path('/rebrand/images/favicon.ico') %>" />
      <link rel="icon" sizes="any" href="<%= assets_path('/rebrand/images/favicon.svg') %>" type="image/svg+xml">
      <link rel="mask-icon" href="<%= assets_path('/rebrand/images/govuk-icon-mask.svg') %>" color="#0b0c0c">
      <link rel="apple-touch-icon" href="<%= assets_path('/rebrand/images/govuk-icon-180.png') %>">
      <link rel="manifest" href="<%= assets_path('/rebrand/manifest.json') %>">
      <meta property="og:image" content="<%= get_subdomain_host(FIND_ENERGY_CERTIFICATE_HOST_NAME) %>/rebrand/images/govuk-opengraph-image.png">
  <% end %>
  <!-- End of Favicon section -->
  <% if ENV["STAGE"] == "test" %>
    <script nonce="<%= script_nonce %>"><%= capybara_lockstep_js %></script>
  <% end %>
<%== erb :partial__include_analytics %>

</head>

<body class="govuk-template__body no-js">
<script nonce="<%= script_nonce %>">
  document.body.classList.remove("no-js");
  document.body.className += ' js-enabled' + ('noModule' in HTMLScriptElement.prototype ? ' govuk-frontend-supported' : '');
</script>
