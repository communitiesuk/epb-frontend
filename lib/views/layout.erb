<section aria-label="Service information" class="govuk-service-navigation"
         data-module="govuk-service-navigation">
  <div class="govuk-width-container">
    <div class="govuk-service-navigation__container">
      <span class="govuk-service-navigation__service-name">
        <a href="<%= localised_url('/') %>" class="govuk-service-navigation__link">
          <%= get_gov_header %>
        </a>
      </span>
    </div>
  </div>
</section>

<div class="govuk-width-container govuk-body">
  <% unless ENV["STAGE"] == "production" %>
    <div class="govuk-phase-banner non-production">
      <p class="govuk-phase-banner__content">
        <strong class="govuk-tag govuk-phase-banner__content__tag epbr-environment-tag">
          <%= ENV["STAGE"].capitalize %>
        </strong>
        <span class="govuk-phase-banner__text">
          <%=t('layout.body.phase_banner')%>
        </span>
      </p>
    </div>
  <% end %>

  <% if @display_language_toggle %>
    <%== erb :_toggle_to_welsh %>
  <% end %>

  <main id="main-content" class="govuk-main-wrapper" role="main">
    <%== yield %>
  </main>
</div>

